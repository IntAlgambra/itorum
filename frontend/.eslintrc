{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb/base",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "no-underscore-dangle": "off",
        "max-classes-per-file": "off",
        "no-param-reassign": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "class-methods-use-this":"off",
        "react/display-name": "off"
    }
}